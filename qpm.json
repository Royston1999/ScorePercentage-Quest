{
  "$schema": "https://raw.githubusercontent.com/QuestPackageManager/QPM.Package/refs/heads/main/qpm.schema.json",
  "version": "0.1.0",
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "Score Percentage",
    "id": "scorepercentage",
    "version": "3.3.4",
    "url": null,
    "additionalData": {
      "overrideSoName": "libscorepercentage.so"
    }
  },
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./build.ps1"
      ],
      "clean": [
        "pwsh ./build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./copy.ps1"
      ],
      "draft": [
        "pwsh ./draftrelease.ps1"
      ],
      "log": [
        "pwsh ./start-logging.ps1"
      ],
      "qmod": [
        "qpm s build",
        "pwsh ./buildQMOD.ps1 ScorePercentage"
      ],
      "restart": [
        "pwsh ./restart-game.ps1"
      ],
      "stack": [
        "pwsh ./ndk-stack.ps1"
      ]
    },
    "ndk": "^27.3.13750724",
    "qmodIncludeDirs": [
      "./build",
      "./extern/libs"
    ],
    "qmodIncludeFiles": [],
    "qmodOutput": "ScorePercentage.qmod"
  },
  "dependencies": [
    {
      "id": "beatsaber-hook",
      "versionRange": "^6.4.2",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.6",
      "additionalData": {
        "includeQmod": false
      }
    },
    {
      "id": "bs-cordl",
      "versionRange": "4008.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.18.3",
      "additionalData": {}
    },
    {
      "id": "config-utils",
      "versionRange": "^2.0.3",
      "additionalData": {}
    },
    {
      "id": "bsml",
      "versionRange": "^0.4.55",
      "additionalData": {}
    },
    {
      "id": "paper2_scotland2",
      "versionRange": "^4.6.4",
      "additionalData": {}
    },
    {
      "id": "metacore",
      "versionRange": "^1.4.0",
      "additionalData": {}
    }
  ]
}
